@model IEnumerable<dynamic>

<div class="container mt-5">
    <h1 class="text-center">Raporlar</h1>
    <h3 class="text-center mt-4">Günlük Çalışan Dolulukları - @ViewBag.Bugun</h3>

    <table class="table table-bordered mt-4">
        <thead class="table-dark">
            <tr>
                <th>Çalışan</th>
                <th>Toplam Çalışma Süresi (dk)</th>
                <th>Toplam Kazanç (TL)</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Calisan</td>
                    <td>@item.ToplamSure</td>
                    <td>@item.ToplamKazanc</td>
                </tr>
            }
        </tbody>
    </table>
</div>
