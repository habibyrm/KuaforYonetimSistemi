@model IEnumerable<dynamic>

<div class="container mt-5">
    <h1 class="text-center">Admin Paneli</h1>
    <h3 class="text-center mt-4">Raporlar</h3>

    <h4 class="mt-4">Günlük Toplam Kazanç: <strong>@ViewBag.GunlukKazanc TL</strong></h4>

    <table class="table table-bordered mt-4">
        <thead class="table-dark">
            <tr>
                <th>Çalışan</th>
                <th>Tamamlanan Randevu Sayısı</th>
                <th>Toplam Kazanç (TL)</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Calisan</td>
                    <td>@item.TamamlananRandevuSayisi</td>
                    <td>@item.ToplamKazanc</td>
                </tr>
            }
        </tbody>
    </table>
</div>
